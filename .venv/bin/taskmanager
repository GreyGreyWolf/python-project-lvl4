#!/Users/greygreywolf/Documents/project/.venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'greywolf-task-manager','console_scripts','taskmanager'
__requires__ = 'greywolf-task-manager'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('greywolf-task-manager', 'console_scripts', 'taskmanager')()
    )
